{
  "name": "@sermonflow/api",
  "version": "0.1.0",
  "private": true,
  "description": "SermonFlow API server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "db:studio": "prisma studio",
    "swagger": "ts-node -r tsconfig-paths/register src/utils/swagger.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
    "@aws-sdk/s3-request-presigner": "^3.427.0",
    "@openai/api": "^1.0.1",
    "@prisma/client": "^5.4.2",
    "@sermonflow/ai": "workspace:*",
    "@sermonflow/types": "workspace:*",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.6.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.1",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.6",
    "openai": "^4.12.1",
    "pino": "^8.16.0",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.3",
    "redis": "^4.6.10",
    "sanitize-html": "^2.11.0",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.8",
    "@types/node": "^18.16.18",
    "@types/nodemailer": "^6.4.11",
    "@types/sanitize-html": "^2.9.0",
    "@types/supertest": "^2.0.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "@types/uuid": "^9.0.5",
    "eslint": "^8.42.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.4.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
